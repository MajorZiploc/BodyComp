{
  "editor.fontSize": 13,
  "editor.fontFamily": "L- Consolas, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.rulers": [120],
  "prettier.printWidth": 120,
  "vim.textwidth": 120,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.parameterHints.enabled": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.minimap.enabled": false,
  "editor.accessibilitySupport": "off",
  "window.menuBarVisibility": "default",
  "workbench.editor.revealIfOpen": true,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "debug.onTaskErrors": "abort",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "files.trimTrailingWhitespace": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "FSharp.fsacRuntime": "netcore",
  "FSharp.useSdkScripts": true,
  "FSharp.suggestGitignore": false,
  "vim.hlsearch": true,
  "vim.easymotion": true,
  "vim.timeout": 400,
  "vim.visualstar": true,
  "vim.useCtrlKeys": false,
  "vim.handleKeys": {
    "<C-j>": true,
    "<C-k>": true,
    "<C-a>": true
  },
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [{ "before": ["k", "j"], "after": ["<Esc>"] }],
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["H"], "after": ["^"] },
    { "before": ["L"], "after": ["$"] },
    { "before": ["U"], "after": ["<C-r>"] },
    { "before": ["Y"], "after": ["y", "$"] },
    { "before": ["<C-j>"], "after": ["<C-d>"] },
    { "before": ["<C-k>"], "after": ["<C-u>"] },
    { "before": ["<C-a>"], "after": ["g", "g", "V", "G"] },
    { "before": ["K"], "after": ["h", "s", "<CR>", "<Esc>", "l"] },
    { "before": ["<BS>"], "after": [":", "n", "o", "h", "<CR>", "<BS>"] },
    { "before": ["<leader>", "s"], "after": ["v", "i", "w", "p"] },
    {
      "before": ["<leader>", "h"],
      "after": [],
      "commands": [{ "command": "workbench.action.navigateBack", "args": [] }]
    },
    {
      "before": ["<leader>", "l"],
      "after": [],
      "commands": [
        { "command": "workbench.action.navigateForward", "args": [] }
      ]
    },
    {
      "before": ["<leader>", "q"],
      "after": [],
      "commands": [{ "command": "editor.action.showHover", "args": [] }]
    },
    {
      "before": ["<leader>", "u"],
      "after": [],
      "commands": [{ "command": "editor.action.goToReferences", "args": [] }]
    },
    {
      "before": ["<leader>", "w", "s"],
      "after": [],
      "commands": [{ "command": "workbench.action.splitEditor", "args": [] }]
    },
    {
      "before": ["<leader>", "w", "h"],
      "after": [],
      "commands": [
        { "command": "workbench.action.focusPreviousGroup", "args": [] }
      ]
    },
    {
      "before": ["<leader>", "w", "l"],
      "after": [],
      "commands": [{ "command": "workbench.action.focusNextGroup", "args": [] }]
    },
    {
      "before": ["<leader>", "w", "H"],
      "after": [],
      "commands": [
        { "command": "workbench.action.moveEditorToPreviousGroup", "args": [] }
      ]
    },
    {
      "before": ["<leader>", "w", "L"],
      "after": [],
      "commands": [
        { "command": "workbench.action.moveEditorToNextGroup", "args": [] }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": ["H"], "after": ["^"] },
    { "before": ["L"], "after": ["$"] },
    { "before": ["U"], "after": ["<C-r>"] },
    { "before": ["<C-j>"], "after": ["<C-d>"] },
    { "before": ["<C-k>"], "after": ["<C-u>"] }
  ]
}
